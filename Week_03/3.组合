import java.util.ArrayList;
import java.util.LinkedList;

//leetcode submit region begin(Prohibit modification and deletion)
class Solution {
    private List<List<Integer>> ans;
    private int n;
    private int k;
    public List<List<Integer>> combine(int n, int k) {
        // å›æº¯, æ‰¾åˆ°æ‰€æœ‰å¯èƒ½çš„è§£æ³•
        ans = new LinkedList<>();
        this.n = n;
        this.k = k;
        back(1, new LinkedList<>());
        return ans;
    }
    private void back(int cur, LinkedList<Integer> container) {
        if (container.size() == k) {
            // å¦‚æœå·²ç»æœ‰æ»¡è¶³æ¡ä»¶çš„å…ƒç´ , åˆ™æŠŠé›†åˆåŠ å…¥åˆ°é˜Ÿåˆ—ä¸­
            ans.add(new LinkedList<>(container));
        }

        for (int i = cur; i <= n; i++) {
            container.add(i);
            back(i + 1, container);
            container.removeLast(); // ç§»é™¤æœ€åä¸€ä¸ªå…ƒç´ ,  æ¯”å¦‚ç¬¬ä¸€æ¬¡éå†åé›†åˆä¸­åŒ…å«äº†[1,2], åˆ™æŠŠ2ç§»é™¤[1], ä¸‹ä¸ªæ ‘[1,3], ç§»é™¤3 è¿™æ ·å°±èƒ½ä½¿ç”¨åŒä¸€ä¸ªcontaineræŠŠæ‰€æœ‰çš„å¯èƒ½ç»™éå†å®Œ
        }
    }
}
